<!DOCTYPE html>
<html lang="en"></html>
<head>
    <% include ../header/header %>

    <style>
    	th, td{
    		border-bottom: 1px solid #ddd;
    	}

    	.container{
    		text-align: center;

    	}

	   </style>
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
  
</head>



	<div class="jumbotron">
	  <h1>Database</h1> 
	  
	</div>


	<div class="container">

    <table style="width: 100%;border:0px solid #ddd;table-layout: fixed;">
      <tr>
        <th>Transporter Name</th>
        <th>Gene Name</th>
        <!-- <th>Synonyms</th> -->
      </tr>
    </table>

		<table id="data" style="width: 100%;border:0px solid #ddd;table-layout: fixed;">
  		
  		<% data.forEach(function(data) { %>
  			<tr>
  				<td><a href="/database/<%= data.TransporterID %>"><%= data.TransporterProteinName %></a></td>
  				<td><%= data.TransporterGeneName %></td>
  				<!-- <td><%= data.Synonyms %></td> -->
  			</tr>
  		<% }); %>
		</table>
	  
	</div>



<footer>
    <% include ../footer/footer %>
</footer>

</body>
</html>